
public class Hello_05 {

	public static void main(String[] args) {

		/*
		 * 변수 선언문
		 * 정수형 어떤 값을 기억장소에 저장할수 있도록
		 * 기억장소를 예약하고
		 * 이름을 num1으로 선언한다
		 * 그리고 num1으로 선언된 기억장소에
		 * 정수 30을 보관하라
		 */
		int num1 = 30;
		
		/*
		 * 변수 선언문
		 * 실수형 어떤 값을 기억장소에 저장할수 있도록
		 * 기억장소를 예약하고
		 * 이름을 num2로 선언한다.
		 * 그리고 num2로 선언된 기억장소에
		 * 실수 30.2를 보관하라
		 */
		double num2 = 30.2;

		
		
	}

}
