
public class Hello_15 {

	public static void main(String[] args) {

		int sum = 10;
		
		sum = sum + 20; // sum 변수값을 20증가 시켜라
		sum = sum + 20; // 계속해서 20씩 증가시켜라
		sum = sum + 20; // 계속해서 20씩 증가시켜라
		sum = sum + 20; // 계속해서 20씩 증가시켜라
		sum = sum + 20; // 계속해서 20씩 증가시켜라
		sum = sum + 20; // 계속해서 20씩 증가시켜라
		sum = sum + 20; // 계속해서 20씩 증가시켜라
		// sum 변수값을 20씩 증가시키는 누적연산이 수행
		System.out.println(sum);
		
		int num1 = 0;
		num1 = num1 + 1;
		num1 = num1 + 1;
		num1 = num1 + 1;
		num1 = num1 + 1;
		num1 = num1 + 1;
		num1 = num1 + 1;
		num1 = num1 + 1;
		
		int num2 = 0;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		num2 = num2 * 1;
		
		
		
		
		
		
	}

}
